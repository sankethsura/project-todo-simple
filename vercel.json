{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "DATABASE": "@database"
    },
    "functions": {
      "index.js": {
        "maxDuration": 10
      }
    }
  }
  